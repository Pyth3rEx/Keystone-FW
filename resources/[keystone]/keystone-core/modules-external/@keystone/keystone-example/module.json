{
  "schemaVersion": 1,
  "id": "@acme/keystone-example",
  "name": "Keystone Garage System Example Module",
  "version": "1.0.0",
  "keystone": "^0.0.0",

  "entry": {
    "server": "./server/index.js",
    "client": "./client/index.js",
    "ui": "./ui/manifest.json"
  },

  "dependsOn": {
    "@keystone/core": "^0.0.0"
  },

  "optionalDependsOn": {
    "@keystone/inventory": "^0.3.0"
  },

  "config": {
    "key": "garage",
    "schema": "./config/schema.json"
  },

  "db": {
    "engine": "postgres",
    "migrations": "./migrations",
    "baseline": 0,
    "required": true
  },

  "metadata": {
    "author": "Keystone",
    "license": "GNU v3",
    "repository": "https://github.com/Pyth3rEx/Keystone-FW",
    "description": "Example third party module for the Keystone framework",
    "tags": ["garage", "vehicles", "roleplay"]
  }
}
